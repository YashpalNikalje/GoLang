

package main 
import(
    "fmt"
)
func main() {
    var t, a, b int
    fmt.Scan(&t)
    for t>0 {
        fmt.Scan(&a, &b)
        ans:=0
        for i:=1; ;i++ {
            if i*i >b{
                break
            }
            if i*i<=b && i*i>=a {
                ans+=1
            }
        }
        fmt.Println(ans)
        t--
    }
}
