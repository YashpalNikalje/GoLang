

package main
import(
    "fmt"
)
func main() {
    var n int
    fmt.Scan(&n)
    a:= make([]int, 101)
    for i:=0; i<n; i++ {
        var ele int
        fmt.Scan(&ele)
        a[ele]++
    }
    max:=0
    for i:=0; i<len(a); i++ {
        if a[i]> max {
            max=a[i]
        }
    }
    fmt.Println(n-max)
}
