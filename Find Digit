

package main
import(
    "fmt"
)
func main() {
     var t int
     fmt.Scan(&t)
     
     for i:=0; i<t; i++ {
         var n int
         fmt.Scan(&n)
         
         m:=n
         ct:=0
         
         for m!=0 {
             digit := m%10
             if (digit!=0) && (n%digit ==0) {
                 ct++
             }
             m/=10
         }
         fmt.Println(ct)
     }
}
