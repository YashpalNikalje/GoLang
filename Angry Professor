

package main
import(
    "fmt"
)
func main() {
    var test int
    fmt.Scan(&test)
    
    for i:=0; i<test; i++ {
        var n, k int
    fmt.Scan(&n, &k)
    count:=0
     a:=make([]int, n)
     for i:=0; i<n; i++ {
         fmt.Scan(&a[i])
         if a[i]<=0 {
             count++
         }
     }
     if count >= k {
         fmt.Println("NO")
     }else {
         fmt.Println("YES")
     }
    }
}
