

package main
import(
    "fmt"
    "sort"
)
func main() {
    var n int
    fmt.Scan(&n)
    a:= make([]int,n)
    for i:=0; i<n; i++ {
        fmt.Scan(&a[i])   
    }
    sort.Ints(a)
    for i:=0; i<n; {
        fmt.Println(n-i)
        min:=a[i]
        for i<n && a[i]==min {
            i++
        }
    }
}
