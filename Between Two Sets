// To find the number of all the integers which satisfy two given conditions.
// i)The integers of first given array are all factors of the selected integer i.e. all the numbers in the first array should divide the given integer perfectly.
// ii)The integer being chosen divides all the numbers of second array perfectly.

package main 

import "fmt"

func main() {
    var n,m,c int
    fmt.Scan(&n,&m)
    
    a:= make([]int,n)
    b:= make([]int,m)
    
    for i:=0;i<n;i++ {
        fmt.Scan(&a[i])
    }
    for i:=0;i<m;i++ {
        fmt.Scan(&b[i])
    }
    
    for i:=1;i<=100;i++ {
        factor:=true
        for j:=0;j<n;j++ {
            if i % a[j] !=0 {
                factor = false
                break
            }
        }
        if factor {
            for j:=0;j<m;j++ {
                if b[j] % i !=0 {
                    factor = false
                    break
                }
            }
        }
        if factor {
            c++
        }
    }
    fmt.Println(c)
}
 
